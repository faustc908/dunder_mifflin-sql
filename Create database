   CREATE DATABASE bookmarks OWNER 'dunder-mifflin';

   CREATE TABLE store (
     id INTEGER PRIMARY KEY GENERATED BY DEFAULT AS IDENTITY,
     title TEXT NOT NULL,
     url VARCHAR NOT NULL,
     rating INTEGER NOT NULL,
     description TEXT
   );

  
